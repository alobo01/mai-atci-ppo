{
    "algo": "grpo",
    "checkpoint_interval": 100000,
    "env_id": "CarRacing-v3",
    "gamma": 0.99,
    "grpo_config": {
        "distribution_type": "beta",
        "entropy_coef": 0.005,
        "group_size": 16,
        "kl_coef": 0.01,
        "lr": 0.0001,
        "max_grad_norm": 0.5,
        "minibatch_size": 128,
        "ref_update_interval": 10000,
        "rollout_steps_per_trajectory": 512,
        "update_epochs": 10
    },
    "log_interval": 10000,
    "max_episode_steps": 1000,
    "network_config": {
        "cnn_output_features": 256,
        "mlp_hidden_dims": [
            256
        ],
        "network_type": "cnn"
    },
    "seed": 0,
    "total_steps": 1000000,
    "verbose": false,
    "video_interval": 250000
}